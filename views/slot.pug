
extends layout

block content
  h1= title
  - var image_names = ["A.jpg","b.jpg","c.jpg","flower.jpg","sun.jpg"]
  - for( let i= 0;i<3;i++){
      -const random = Math.floor(Math.random()*5)

      img(src=("/images/"+`${image_names[random]}`) alt=`${image_names[random]}` height:300px width:300px type = " ")
  -}